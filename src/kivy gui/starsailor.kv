#:kivy 2.3.0
#:import utils kivy.utils

<Starsailor>
    cols: 1
    rows: 3
    BoxLayout:
        size_hint: 1, .4
        orientation: "horizontal"
        Button:
            text: "Help"
            background_color: kivy.utils.get_color_from_hex("#8E3F3C")
        Image:
            source: "../../pictures/Starsailor_Banner.png"
            allow_stretch: True
        Button:
            text: "Menu"
            background_color: kivy.utils.get_color_from_hex("#FCEEC7")
    BoxLayout:
        orientation: "horizontal"
        Image:
            source: "../../pictures/coverphoto.jpg"
            size: self.texture_size
    BoxLayout:
        orientation: "horizontal"
        Label:
            text: "Label 6"
        Label:
            text: "Label 7"
        Label:
            text: "Label 8"
        Label:
            text: "Label 9"
        Label:
            text: "Label 10"





